            <element name="result" type="xsd:double" minOccurs="0" maxOccurs="1"/><!-- ns__div::result -->
          </sequence>
      </complexType>
    </element>
    <!-- operation request element -->
    <element name="pow">
      <complexType>
          <sequence>
            <element name="a" type="xsd:double" minOccurs="1" maxOccurs="1"/><!-- ns__pow::a -->
            <element name="b" type="xsd:double" minOccurs="1" maxOccurs="1"/><!-- ns__pow::b -->
          </sequence>
      </complexType>
    </element>
    <!-- operation response element -->
    <element name="powResponse">
      <complexType>
          <sequence>
            <element name="result" type="xsd:double" minOccurs="0" maxOccurs="1"/><!-- ns__pow::result -->
          </sequence>
      </complexType>
    </element>
  </schema>


    </element>
    <!-- operation response element -->
    <element name="subResponse">
      <complexType>
          <sequence>
            <element name="result" type="xsd:double" minOccurs="0" maxOccurs="1"/><!-- ns__sub::result -->
          </sequence>
      </complexType>
    </element>
    <!-- operation request element -->
    <element name="mul">
      <complexType>
          <sequence>
            <element name="a" type="xsd:double" minOccurs="1" maxOccurs="1"/><!-- ns__mul::a -->
            <element name="b" type="xsd:double" minOccurs="1" maxOccurs="1"/><!-- ns__mul::b -->
          </sequence>
      </complexType>
    </element>
    <!-- operation response element -->
    <element name="mulResponse">
      <complexType>
          <sequence>
            <element name="result" type="xsd:double" minOccurs="0" maxOccurs="1"/><!-- ns__mul::result -->
          </sequence>
      </complexType>
    </element>
    <!-- operation request element -->
    <element name="div">
      <complexType>
          <sequence>
            <element name="a" type="xsd:double" minOccurs="1" maxOccurs="1"/><!-- ns__div::a -->
